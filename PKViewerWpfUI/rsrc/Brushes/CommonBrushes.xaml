<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:p="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">

    <!--#region FREEZED COLORS-->
    <Color x:Key="Colorless" p:Freeze="True">Transparent</Color>
    <Color x:Key="BlackColor" p:Freeze="True">Black</Color>
    <Color x:Key="GrayColor" p:Freeze="True">Gray</Color>
    <Color x:Key="WhiteColor" p:Freeze="True">WhiteSmoke</Color>
    <Color x:Key="BlueColor" p:Freeze="True">#0039a6</Color>
    <Color x:Key="RedColor" p:Freeze="True">#d52b1e</Color>
    <Color x:Key="LightGreenColor" p:Freeze="True">LightGreen</Color>
    <Color x:Key="GreenColor" p:Freeze="True">Green</Color>
    <Color x:Key="YellowColor" p:Freeze="True">Yellow</Color>
    <Color x:Key="DeepSkyBlueColor" p:Freeze="True">DeepSkyBlue</Color>

    <Color x:Key="IconRedColor" p:Freeze="True">#db554a</Color>
    <Color x:Key="IconYellowColor" p:Freeze="True">#fdff32</Color>
    <Color x:Key="IconGreenColor" p:Freeze="True">#319932</Color>
    <Color x:Key="IconGrayColor" p:Freeze="True">#979998</Color>
    <Color x:Key="IconDisabledGrayColor" p:Freeze="True">#b5b9b7</Color>
    <!--#endregion-->

    <!--#region FREEZED BRUSHES-->
    <SolidColorBrush x:Key="ColorlessBrush" Color="{StaticResource Colorless}" p:Freeze="True"/>
    <SolidColorBrush x:Key="GrayBrush" Color="{StaticResource GrayColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource WhiteColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="BlueBrush" Color="{StaticResource BlueColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="RedBrush" Color="{StaticResource RedColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="LightGreenBrush" Color="{StaticResource LightGreenColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="GreenBrush" Color="{StaticResource GreenColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="YellowBrush" Color="{StaticResource YellowColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="DeepSkyBlueBrush" Color="{StaticResource DeepSkyBlueColor}" p:Freeze="True"/>

    <SolidColorBrush x:Key="IconRedBrush" Color="{StaticResource IconRedColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="IconYellowBrush" Color="{StaticResource IconYellowColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="IconGreenBrush" Color="{StaticResource IconGreenColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="IconGrayBrush" Color="{StaticResource IconGrayColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="IconDisabledGrayBrush" Color="{StaticResource IconDisabledGrayColor}" p:Freeze="True"/>
    <!--#endregion-->
</ResourceDictionary>