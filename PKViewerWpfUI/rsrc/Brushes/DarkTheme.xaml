<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:p="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">

    <!--#region FREEZED COLORS-->
    <Color x:Key="PopupBgColor" p:Freeze="True">#D0000000</Color>
    <Color x:Key="PopupFgColor" p:Freeze="True">AliceBlue</Color>
    <Color x:Key="PanelBorderColor" p:Freeze="True">LightGray</Color>
    <Color x:Key="HighlightColor" p:Freeze="True">Blue</Color>
    <Color x:Key="TopPanelBgColor" p:Freeze="True">#1603aa</Color>
    <Color x:Key="MainPanelBgColor" p:Freeze="True">#130e9b</Color>
    <Color x:Key="RightPanelBgColor" p:Freeze="True">#0f0b75</Color>
    <Color x:Key="ButtonBgColor" p:Freeze="True">#4d4d4d</Color>
    <Color x:Key="MainGridLinesColor" p:Freeze="True">#969696</Color>
    <Color x:Key="MainGridOddItemsColor" p:Freeze="True">#0d0bb3</Color>
    <Color x:Key="TextColor" p:Freeze="True">WhiteSmoke</Color>
    <Color x:Key="ThemeSwitcherMoonColor" p:Freeze="True">Yellow</Color>
    <Color x:Key="ProgressRingFgColor" p:Freeze="True">#e0ffff</Color>
    <!--#endregion-->

    <!--#region FREEZED BRUSHES-->
    <SolidColorBrush x:Key="PopupBgBrush" Color="{StaticResource PopupBgColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="PopupFgBrush" Color="{StaticResource PopupFgColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="PopupBorderBrush" Color="{StaticResource BlackColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="MainGridLinesBrush" Color="{StaticResource MainGridLinesColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="MainGridOddItemsBrush" Color="{StaticResource MainGridOddItemsColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="ButtonBgBrush" Color="{StaticResource ButtonBgColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="TextBrush" Color="{StaticResource WhiteColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="PanelBorderBrush" Color="{StaticResource PanelBorderColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="TopPanelBgBrush" Color="{StaticResource TopPanelBgColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="MainPanelBgBrush" Color="{StaticResource MainPanelBgColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="RightPanelBgBrush" Color="{StaticResource RightPanelBgColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="ProgressRingBrush" Color="{StaticResource ProgressRingFgColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="ThemeSwitcherMainBorderBrush" Color="{StaticResource WhiteColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="ThemeSwitcherLeftBorderBrush" Color="{StaticResource Colorless}" p:Freeze="True"/>
    <SolidColorBrush x:Key="ThemeSwitcherLeftBorderBgBrush" Color="{StaticResource Colorless}" p:Freeze="True"/>
    <SolidColorBrush x:Key="ThemeSwitcherRightBorderBrush" Color="{StaticResource WhiteColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="ThemeSwitcherMoonBrush" Color="{StaticResource ThemeSwitcherMoonColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="ThemeSwitcherSunBrush" Color="{StaticResource ThemeSwitcherMoonColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="HighlightBrush" Color="{StaticResource HighlightColor}" p:Freeze="True"/>
    <!--#endregion-->
</ResourceDictionary>