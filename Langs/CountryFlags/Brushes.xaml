<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:p="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    <Color x:Key="Colorless" p:Freeze="True">Transparent</Color>
    <Color x:Key="BlackColor" p:Freeze="True">Black</Color>
    <Color x:Key="GrayColor" p:Freeze="True">Gray</Color>
    <Color x:Key="WhiteColor" p:Freeze="True">WhiteSmoke</Color>
    <Color x:Key="BlueColor" p:Freeze="True">#0039a6</Color>
	<Color x:Key="RedColor" p:Freeze="True">#d52b1e</Color>
	<Color x:Key="DarkOrangeColor" p:Freeze="True">DarkOrange</Color>

	<SolidColorBrush x:Key="ColorlessBrush" Color="{StaticResource Colorless}" p:Freeze="True"/>
    <SolidColorBrush x:Key="GrayBrush" Color="{StaticResource GrayColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource WhiteColor}" p:Freeze="True"/>
    <SolidColorBrush x:Key="BlueBrush" Color="{StaticResource BlueColor}" p:Freeze="True"/>
	<SolidColorBrush x:Key="RedBrush" Color="{StaticResource RedColor}" p:Freeze="True"/>
	<SolidColorBrush x:Key="DarkOrangeBrush" Color="{StaticResource DarkOrangeColor}" p:Freeze="True"/>

</ResourceDictionary>